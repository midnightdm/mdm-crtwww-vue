    "express": "^4.17.1",
    "firebase": "^9.0.0",
    "moment": "^2.29.1",
    "serve-static": "^1.14.1",
    "vue": "^3.2.6",
    "vue-router": "^4.0.0-0",
    "vue-svg-loader": "^0.10.0",
    "vue-template-compiler": "^2.0.0",
